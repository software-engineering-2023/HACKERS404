<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>Admin - Bank Finance</title>
  <!-- Add your CSS stylesheets and other external dependencies here -->
</head>
<body>
  <h1>Admin - Bank Finance</h1>
  
  <h2>Money Income vs Outcome</h2>
  <table>
    <tr>
      <th>Category</th>
      <th>Income</th>
      <th>Outcome</th>
    </tr>
    <% financeData.forEach(data => { %>
      <tr>
        <td><%= data.category %></td>
        <td><%= data.income %></td>
        <td><%= data.outcome %></td>
      </tr>
    <% }) %>
  </table>

  <!-- Additional features: -->
  <h2>Bank Reports</h2>
  <!-- Add code to display additional bank reports, such as profit/loss statements, balance sheets, etc. -->
</body>
</html>
